<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css">
    <title>Track</title>
</head>

<body>
    <table class="table table-bordered col-sm-10 offset-1">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">User</th>
                <th scope="col">Supplied Salt</th>
                <th scope="col">Supplied Hash</th> 
                <th scope="col">Expected Hash</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(function (e, i) { %>
            <tr>
                <th scope="row"><%= i+1 %></th>
                <td><%= e.user %></td>
                <td><%= e.supplied_salt %></td>
                <td><%= e.supplied_hash %></td>
                <td><%= e.expected_hash %></td>
            </tr>
            <% }); %>
        </tbody>
    </table>
</body>

</html>